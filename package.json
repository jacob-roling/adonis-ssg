{
  "name": "adonis-ssg",
  "version": "1.0.8",
  "description": "Prerender routes to static HTML for AdonisJS",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "scripts": {
    "build": "unbuild"
  },
  "keywords": [
    "adonisjs",
    "adonis",
    "ace",
    "cli",
    "ssg",
    "prerender"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "author": "Jacob Roling <admin@composedigital.agency> (https://composedigital.agency)",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/core": "^5.9.0",
    "@types/node": "^20.4.1",
    "unbuild": "^1.2.1"
  },
  "peerDependencies": {
    "@adonisjs/core": "^5.9.0"
  },
  "files": [
    "dist"
  ],
  "adonisjs": {
    "commands": [
      "adonis-ssg/dist/index.cjs"
    ]
  }
}
